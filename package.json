{
  "name": "jiujiu-plugin",
  "version": "1.2.0",
  "description": "Yunzai-Bot 多功能图片插件 - 提供本地/网络随机图片、智能缓存和自动更新",
  "main": "apps/main.js",
  "type": "module",
  "scripts": {
    "dev": "NODE_ENV=development node --watch apps/main.js --self-check",
    "start": "node apps/main.js",
    "test": "NODE_ENV=test jest --coverage",
    "test:watch": "jest --watch",
    "lint": "eslint . --ext .js,.ts",
    "lint:fix": "eslint . --ext .js,.ts --fix",
    "format": "prettier --write \"**/*.{js,ts,json,md}\"",
    "prepare": "husky install",
    "release": "standard-version",
    "build": "tsc && npm run format"
  },
  "keywords": [
    "yunzai",
    "qqbot",
    "plugin",
    "image",
    "random"
  ],
  "author": "YourName <your.email@example.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourname/jiujiu-plugin.git"
  },
  "bugs": {
    "url": "https://github.com/yourname/jiujiu-plugin/issues"
  },
  "homepage": "https://github.com/yourname/jiujiu-plugin#readme",
  "engines": {
    "node": ">=16.0.0",
    "yunzai": ">=3.0.0"
  },
  "dependencies": {
    "node-fetch": "^3.3.0",
    "lru-cache": "^7.14.0",
    "file-type": "^17.1.0",
    "sqlite3": "^5.0.11",
    "sqlite": "^4.1.2",
    "semver": "^7.3.8",
    "chalk": "^4.1.2",
    "dayjs": "^1.11.7"
  },
  "devDependencies": {
    "@types/jest": "^29.0.0",
    "@types/node": "^18.0.0",
    "@types/sqlite3": "^3.1.8",
    "@typescript-eslint/eslint-plugin": "^5.36.0",
    "@typescript-eslint/parser": "^5.36.0",
    "eslint": "^8.23.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^27.0.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.0.1",
    "eslint-plugin-unicorn": "^43.0.0",
    "husky": "^8.0.1",
    "jest": "^29.0.0",
    "lint-staged": "^13.0.0",
    "prettier": "^2.7.1",
    "standard-version": "^9.5.0",
    "ts-jest": "^29.0.0",
    "typescript": "^4.8.0"
  },
  "peerDependencies": {
    "yunzai-core": "^3.0.0",
    "oicq": "^2.0.0"
  },
  "lint-staged": {
    "*.{js,ts}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  },
  "jest": {
    "testEnvironment": "node",
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/tests/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "collectCoverageFrom": [
      "apps/**/*.{js,ts}",
      "!**/node_modules/**",
      "!**/tests/**"
    ]
  }
}